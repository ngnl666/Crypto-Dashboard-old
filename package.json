{
	"name": "crypto-dashboard",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"test:unit": "vitest watch",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --open --coverage",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint:eslint": "eslint --fix --ext .js,.ts,.cjs,.vue ./src",
		"lint:prettier": "prettier --write --loglevel warn \"*.{js,ts,cjs,json,tsx,css,less,scss,vue,html,md}\"",
		"lint:lint-staged": "lint-staged",
		"commit": "git pull && git add -A && git-cz"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.0.9",
		"@vueuse/core": "^9.3.0",
		"axios": "^0.27.2",
		"echarts": "^5.4.0",
		"element-plus": "^2.2.17",
		"pinia": "^2.0.22",
		"pinia-plugin-persist": "^1.0.0",
		"vue": "^3.2.42",
		"vue-echarts": "^6.2.3",
		"vue-router": "^4.1.5",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@iconify-json/ep": "^1.1.8",
		"@iconify-json/ic": "^1.1.9",
		"@iconify-json/mdi": "^1.1.33",
		"@types/node": "^18.8.5",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vitest/browser": "^0.25.1",
		"@vitest/coverage-c8": "^0.25.1",
		"@vitest/ui": "^0.25.1",
		"@vue/test-utils": "^2.1.0",
		"autoprefixer": "^10.4.12",
		"commitizen": "^4.2.5",
		"cz-git": "^1.3.11",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.5.1",
		"husky": "^8.0.1",
		"jsdom": "^20.0.1",
		"lint-staged": "^13.0.3",
		"postcss": "^8.4.17",
		"postcss-html": "^1.5.0",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"rollup-plugin-visualizer": "^5.8.2",
		"sass": "^1.55.0",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.6.4",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-icons": "^0.14.11",
		"unplugin-vue-components": "^0.22.7",
		"vite": "^3.1.0",
		"vite-plugin-html": "^3.2.0",
		"vitest": "^0.23.4",
		"vue-tsc": "^1.0.6"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	}
}
